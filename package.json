{
  "name": "draft-js-emoji-plugin",
  "version": "2.1.1",
  "description": "Emoji Plugin for DraftJS",
  "author": {
    "name": "Nik Graf",
    "email": "nik@nikgraf.com",
    "url": "http://www.nikgraf.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/etac1992/draft-js-emoji-plugin.git"
  },
  "main": "lib/index.js",
  "keywords": [
    "editor",
    "wysiwyg",
    "draft",
    "react",
    "ux",
    "components",
    "widget",
    "react-component"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "build:js": "WEBPACK_CONFIG=$(pwd)/webpack.config.js BABEL_DISABLE_CACHE=1 BABEL_ENV=production NODE_ENV=production babel --out-dir='lib' --ignore='__test__/*' src",
    "build:css": "node ./scripts/concatCssFiles $(pwd) && rimraf lib-css",
    "prepublish": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "decorate-component-with-props": "^1.1.0",
    "emojione": "^2.2.7",
    "find-with-regex": "^1.1.3",
    "immutable": "~3.7.4",
    "prop-types": "^15.5.8",
    "lodash.keys": "^4.2.0",
    "react-custom-scrollbars": "^4.2.0",
    "react-icons": "^2.2.6",
    "to-style": "^1.3.3",
    "union-class-names": "^1.0.0",
    "draft-js": "^0.10.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",

    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "postcss-loader": "^1.3.3",

    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1"
  },
  "peerDependencies": {
    "draft-js": "^0.10.1",
    "react": "^15.5.0 || ^16.0.0-rc",
    "react-dom": "^15.5.0 || ^16.0.0-rc"
  }
}
